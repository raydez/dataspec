{
  "name": "@raydez/dataspec",
  "version": "0.1.0",
  "description": "AI-native tool for data development teams",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dataspec": "./bin/dataspec.js"
  },
  "scripts": {
